{
  "preset": "ts-jest",
  "globals": {
    "ts-jest": {
      "tsConfig": "tsconfig.json"
    }
  },
  "collectCoverageFrom": [
    "**/*.{tsx,ts}"
  ],
  "setupFiles": [
    "<rootDir>/setupTests.ts"
  ],
  "moduleNameMapper": {
    "^.+\\.css$": "identity-obj-proxy",
    "^.+\\.scss$": "identity-obj-proxy",
    "^.+\\.svg$": "identity-obj-proxy"
  },
  "testPathIgnorePatterns": [
    "node_modules",
    "__mocks__"
  ]
}
